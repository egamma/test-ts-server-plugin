{
    "rules": {
        "arrow-parens": true,
        "no-var-keyword": true,
        // "no-unused-variable": [
        //     true,
        //     {
        //         "ignore-pattern": "^_"
        //     }
        // ],
        "no-trailing-whitespace": true,
        "quotemark": [
            true,
            "double"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ]
    },
    "jsRules": {
        "triple-equals": [
            true,
            "allow-null-check"
        ]
    },
    "defaultSeverity": "warning"
}